<script src="{{ asset('vendor/vendor/jquery-steps/jquery.steps.min.js') }}"></script>
{# the JS file lives at "public/vendor/vendor/minimalist-picker/dobpicker.js" #}
<script src="{{ asset('vendor/vendor/minimalist-picker/dobpicker.js') }}"></script>

{# the JS file lives at "public/vendor/vendor/nouislider/nouislider.min.js" #}
<script src="{{ asset('vendor/vendor/nouislider/nouislider.min.js') }}"></script>
{# the JS file lives at "public/vendor/vendor/wnumb/wNumb.js" #}
<script src="{{ asset('vendor/vendor/wnumb/wNumb.js') }}"></script>
{# the JS file lives at "public/js/main5.jss" #}
<script src="{{ asset('js/main5.js') }}"></script>


</body>

</html>
{% endblock %}
